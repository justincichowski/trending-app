# This is an example environment file for the server.
# Copy this file to .env and fill in the values.

# The port the Fastify server will listen on.
PORT=3000

# YouTube API Key
# This is required for the Cooking and Travel categories.
YOUTUBE_API_KEY=

# Optional: YouTube Playlist IDs
# If these are not set, the YouTube service will fall back to a search query.
COOKING_PLAYLIST_ID=
TRAVEL_PLAYLIST_ID=