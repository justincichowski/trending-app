{
	"compilerOptions": {
		"target": "es2020",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": ".vercel/output/functions",
		"types": ["node"]
	},
	"include": ["**/*.ts"],
	"exclude": ["node_modules", "**/*.test.ts"]
}
